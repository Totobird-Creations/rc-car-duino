[gd_resource type="Shader" format=2]

[resource]
code = "shader_type canvas_item;



uniform float angle = 0.0;



void fragment() {
	if (
		distance(UV, vec2(0.5)) < 0.5 &&
		distance(UV, vec2(0.5)) > 0.25 &&
		distance(UV, vec2(cos(angle), sin(angle))) > 0.5
	) {
		COLOR = vec4(1.0);
	} else {
		COLOR = vec4(0.0);
	}
}
"
